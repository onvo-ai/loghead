{
  "nodeModulesDir": "auto",
  "tasks": {
    "start": "deno run --allow-net --allow-read --allow-write --allow-env --allow-run --allow-ffi src/main.ts",
    "dev": "deno run --watch --allow-net --allow-read --allow-write --allow-env --allow-run --allow-ffi src/main.ts",
    "build": "deno compile --allow-net --allow-read --allow-write --allow-env --allow-run --allow-ffi --output build/loggerhead src/main.ts",
    "db:migrate": "deno run --allow-net --allow-read --allow-write --allow-env --allow-run --allow-ffi src/db/migrate.ts",
    "test": "deno test --allow-read --allow-write --allow-env --allow-run --allow-ffi"
  },
  "imports": {
    "@std/dotenv": "jsr:@std/dotenv@^0.225.2",
    "@std/flags": "jsr:@std/flags@^0.224.0",
    "@std/log": "jsr:@std/log@^0.224.0",
    "@std/cli": "jsr:@std/cli@^1.0.6",
    "@std/streams": "jsr:@std/streams@^1.0.8",
    "@std/fmt": "jsr:@std/fmt@^1.0.0",
    "@std/path": "jsr:@std/path@^1.0.0",
    "@std/fs": "jsr:@std/fs@^1.0.0",
    "@std/assert": "jsr:@std/assert@^1.0.0",
    "@modelcontextprotocol/sdk": "npm:@modelcontextprotocol/sdk@^1.0.1",
    "ollama": "npm:ollama@^0.5.9",
    "@db/sqlite": "jsr:@db/sqlite@^0.12.0",
    "sqlite-vec": "npm:sqlite-vec@0.1.7-alpha.2",
    "sqlite-vec-darwin-arm64": "npm:sqlite-vec-darwin-arm64@0.1.7-alpha.2",
    "@cliffy/ansi": "jsr:@cliffy/ansi@^1.0.0-rc.7",
    "@cliffy/table": "jsr:@cliffy/table@^1.0.0-rc.7"
  }
}
